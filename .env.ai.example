# AI Configuration
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key_here
VITE_GOOGLE_API_KEY=your_google_api_key_here
VITE_AZURE_OPENAI_ENDPOINT=https://your-azure-openai-endpoint.openai.azure.com/
VITE_AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here
VITE_AZURE_OPENAI_VERSION=2024-02-15-preview
VITE_COHERE_API_KEY=your_cohere_api_key_here

# Ollama Configuration (Local AI)
VITE_OLLAMA_ENDPOINT=http://localhost:11434
VITE_OLLAMA_MODEL=llama3.2:3b

# Vector Database Configuration  
VITE_QDRANT_ENDPOINT=http://localhost:6333
VITE_QDRANT_API_KEY=your_qdrant_api_key_here

# AI Features
VITE_ENABLE_AI_ASSISTANT=true
VITE_ENABLE_COST_TRACKING=true
VITE_ENABLE_PROVIDER_HEALTH_CHECK=true
VITE_DEFAULT_AI_PROVIDER=ollama

# Performance Settings
VITE_AI_REQUEST_TIMEOUT=30000
VITE_AI_MAX_RETRIES=3
VITE_AI_BATCH_SIZE=10
